// 5-Escreva um programa que recebe o nome de um mês e imprime o número de dias daquele mês. Considere um ano não bissexto.

var Diasdomes = parseInt(prompt("Quantos dias tem o seguinte mês));

if (Diasdomes == "Janeiro") {
    alert("31 dias");
} else if (Diasdomes == Setembro"){
    alert("30 dias");
}
